{
    extends: "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "semi": 2,
        "quotes": [
          2,
          "single"
        ]
    }
}
