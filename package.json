    "mkdirp": "0.5.1",
    "mocha": "^2.4.5",
    "nodemon": "^1.8.1",
    "npm-run-all": "^1.5.1",
    "require-dir": "0.3.0",
    "tap-spec": "4.1.1",
    "tape": "4.2.2"
  },
  "dependencies": {
    "babel-core": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "body-parser": "^1.14.1",
    "express": "^4.13.3",
    "fstream": "^1.0.8",
