{
  "name": "apidoc-javascript-generator",
  "version": "1.4.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "rm -rf dist && npm-run-all build:*",
    "build:lib": "babel lib --out-dir dist/lib",
    "build:server": "babel server.js --out-dir dist",
    "build:config": "cp package.json generators.json dist/",
    "build:assets": "cp -R public dist/public",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --recursive",
    "dev": "nodemon"
  },
  "author": "Chris Hicks <chicks@chrhicks.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "6.16.0",
    "eslint": "3.6.1",
    "expect": "^1.14.0",
    "gulp": "3.9.1",
    "mkdirp": "0.5.1",
    "mocha": "3.1.0",
    "nodemon": "^1.8.1",
    "npm-run-all": "3.1.0",
    "require-dir": "0.3.0",
    "tap-spec": "4.1.1",
    "tape": "4.6.1"
  },
  "dependencies": {
    "@flowio/lib-apidoc": "1.5.21",
    "babel-core": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "body-parser": "^1.14.1",
    "express": "^4.13.3",
    "fstream": "^1.0.8",
    "handlebars": "^4.0.3",
    "hbs": "4.0.1",
    "lodash": "4.16.2",
    "mkdirp": "^0.5.1",
    "tar": "^2.2.1",
    "tar.gz": "^1.0.3"
  }
